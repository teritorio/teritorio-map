{
  "name": "@teritorio/map",
  "version": "0.13.0",
  "packageManager": "yarn@4.6.0",
  "license": "MIT",
  "keywords": [
    "mapbox",
    "maplibre",
    "teritorio",
    "gis",
    "vector",
    "gl"
  ],
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "dist/teritorio-map.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --format cjs,esm,umd --name teritorio",
    "build": "tsdx build --format cjs,esm,umd --name teritorio",
    "test": "tsdx test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks && tsdx build --format cjs,esm,umd --name teritorio"
  },
  "peerDependencies": {
    "mapbox-gl": "^1.10.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.4.3",
    "maplibre-gl": "~5.1.0",
    "simple-git-hooks": "^2.11.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^5.0.0"
  },
  "simple-git-hooks": {
    "commit-msg": "yarn dlx commitlint --edit $1",
    "pre-commit": "yarn dlx lint-staged",
    "preserveUnused": true
  },
  "lint-staged": {
    "*": "yarn lint:fix"
  }
}
